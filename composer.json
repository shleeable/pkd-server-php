{
  "name": "fedi-e2ee/pkd-server",
  "autoload": {
    "files": [
      "autoload.php"
    ],
    "psr-4": {
      "FediE2EE\\PKDServer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FediE2EE\\PKDServer\\Tests\\": "tests/"
    }
  },
  "license": "ISC",
  "type": "project",
  "require": {
    "php": "^8.4",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-sodium": "*",
    "fedi-e2ee/pkd-crypto": "^0.22|1",
    "fedi-e2ee/pkd-extensions": "^0.2|^1",
    "guzzlehttp/guzzle": "^7",
    "laminas/laminas-diactoros": "^3",
    "laminas/laminas-httphandlerrunner": "^2",
    "landrok/activitypub": "^0",
    "league/route": "^6",
    "monolog/monolog": "^3.9",
    "paragonie/certainty": "^3",
    "paragonie/ciphersweet": "^4.9",
    "paragonie/constant_time_encoding": "^3",
    "paragonie/easy-ecc": "^1",
    "paragonie/easydb": "^3",
    "paragonie/easydb-cache": "^2.2",
    "predis/predis": "^3",
    "psr/http-message": "^2",
    "psr/simple-cache": "^3",
    "twig/twig": "^3",
    "ulrichsg/getopt-php": "^4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.5",
    "nikic/php-fuzzer": "^0.0.11",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12",
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "^6.14"
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
